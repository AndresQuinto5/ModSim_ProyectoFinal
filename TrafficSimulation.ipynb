    "                            gap += 1\n",
    "                        else:\n",
    "                            break\n",
    "                        looked_ahead += 1\n",
    "\n",
    "\n",
    "                    if vel > gap:                     #rule 2 - reduce speed to gap\n",
    "                        vel = max(gap, 0)\n",
    "\n",
    "                                                    #rule 3 - randomness\n",
    "                    vel = max(vel - 1, 0) if rd.random() < self.prob_slowing else vel \n",
    "\n",
    "                    lane[spot] = vel                  #update speeds\n",
    "\n",
